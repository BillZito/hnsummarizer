{
	"manifest_version": 2,
	"name": "HN Preview",
	"short_name": "HN previewer to help you pick meaningful links",
	"version": "1.0.1",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>", "file://*"
			],
			"js": ["node_modules/jquery/dist/jquery.js", "content.js"]
		}
	],
	"browser_action": {
		"default_icon": "icon16.png"
	},
	"icons": {"48": "icon128.png",
			  "128": "icon128.png"},
	"background": {
		"scripts": ["background.js"]
	}
}